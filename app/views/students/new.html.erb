<!-- Create your form here -->
<h3>Student Form</h3>
	<%= form_tag students_path do %>
		<p><label for = 'student[first_name]'>First Name:</label>
		<%= text_field_tag :'student[first_name]' %></p>
		<p><label>Last Name:</label>
		<%= text_field_tag :'student[last_name]' %></p>
		<%= submit_tag "Submit Student" %>
	<% end %>